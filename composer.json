{
	"name": "phonectas/async-soap-client",
	"description": "A SOAP client that can do Asynchronous SOAP calls",
	"keywords": ["phonect", "soap", "client"],
	"license": "Copyright (c) 2017 Phonect AS",
	"type": "library",
	"require": {
		"php": ">=5.5.9",
		"guzzlehttp/guzzle": "^6.2",
		"monolog/monolog": "^1.23",
		"guzzlehttp/streams": "^3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.8"
	},
	"autoload": {
		"psr-0": {
			"Phonect": "src"
		}
	},
	"scripts": {
		"syntax": "bash ./phplint.sh",
		"test": "bash vendor/bin/phpunit -c phpunit.xml.dist",
		"post-install-cmd": "vendor/bin/phpunit -c phpunit.xml.dist"
	}
}
